#!/bin/bash
DOMAIN=${1:-magento2.test}
VERSION=${2:-2.3.0}
BYPASS=${3:-no}

git clone https://github.com/freeyland/docker-magento.git ./

#!/bin/bash
git init -qqq
git remote add origin https://github.com/freeyland/docker-magento
git fetch origin -qqq
git checkout origin/master
rm -rf .git
git init

if [ $BYPASS != "yes" ]; 
then
bin/download $VERSION
fi

bin/setup $DOMAIN
